{
	"presets": [
		"@babel/preset-react",
		[
			"@babel/preset-env", {
				"targets": {
					"browsers": ["last 2 versions", "safari >= 7"]
				}
			}
		]
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-proposal-object-rest-spread"
	],
	"env": {
		"production": {
			"presets": [
				"@babel/preset-react",
				[
					"@babel/preset-env", {
					"targets": {
						"browsers": ["last 2 versions", "safari >= 7"]
					}
				}
				]
			],
			"plugins": [
				"@babel/plugin-proposal-class-properties",
				"@babel/plugin-proposal-object-rest-spread",
				"transform-react-remove-prop-types"
			]
		}
	}
}
