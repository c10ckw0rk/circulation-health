{
	"env": {
		"production": {
			"presets": [
				"@babel/preset-react",
				[
					"@babel/preset-env", {
					"targets": {
						"browsers": ["last 2 versions", "safari >= 7"]
					}
				}
				]
			],
			"plugins": [
				"@babel/plugin-proposal-class-properties",
				"@babel/plugin-proposal-object-rest-spread",
			]
		},
		"development": {
			"presets": [
				"@babel/preset-react",
				[
					"@babel/preset-env", {
					"targets": {
						"browsers": ["last 2 versions", "safari >= 7"]
					}
				}
				]
			],
			"plugins": [
				"@babel/plugin-proposal-class-properties",
				"@babel/plugin-proposal-object-rest-spread"
			]
		}
	}
}
